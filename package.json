{
  "name": "lab-sync",
  "version": "1.0.0",
  "private": true,
  "description": "Lab Sync - Making Science Easier. Simple lab management for research teams.",
  "scripts": {
    "dev": "next dev",
    "build": "npm run validate-config && next build",
    "build:analyze": "ANALYZE=true npm run build",
    "build:standalone": "BUILD_STANDALONE=true npm run build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit --incremental false",
    "validate-config": "node validate-config.cjs",
    "test:build": "npm run validate-config && npm run type-check && npm run lint && npm run build",
    "clean": "rm -rf .next node_modules/.cache",
    "clean:all": "rm -rf .next node_modules package-lock.json && npm install",
    "postinstall": "npm run validate-config || true",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
